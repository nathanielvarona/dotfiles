{
  // "workbench.settings.editor": "json",
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.preferredDarkColorTheme": "GitHub Dark Default",
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
  "workbench.startupEditor": "readme",
  "workbench.iconTheme": "file-icons",
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#c9d1d9",
    "editorCursor.background": "#0d1117"
  },
  "window.autoDetectColorScheme": true,
  "explorer.compactFolders": false,
  "git.autofetch": true,
  "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, 'Symbols Nerd Font'",
  "editor.fontSize": 14,
  "editor.fontLigatures": false,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "terminal.integrated.fontFamily": "'JetBrains Mono NL', 'Symbols Nerd Font'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.macOptionIsMeta": true,
  "go.toolsManagement.autoUpdate": true,
  "projectManager.git.baseFolders": ["~/Projects"],
  "projectManager.sortList": "Recent",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": true
  },
  "shellformat.path": "/usr/local/bin/shfmt",
  "shellformat.flag": "--indent 2",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "find-it-faster.general.useTerminalInEditor": true,
  "find-it-faster.general.killTerminalAfterUse": true,
  "find-it-faster.general.batTheme": "GitHub",
  "editor.lineNumbers": "relative",
  "settings.cycle": [
    {
      "id": "relativeLineNumbers",
      "overrideWorkspaceSettings": false,
      "values": [
        {
          "editor.lineNumbers": "on"
        },
        {
          "editor.lineNumbers": "relative"
        }
      ]
    }
  ],
  "todo-tree.highlights.enabled": false,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.general.tags": [
    "NOTE: ",
    "DEBUG: ",
    "REVIEW: ",
    "HACK: ",
    "QUESTION: ",
    "TODO: ",
    "CHANGED: ",
    "OPTIMIZE: ",
    "FIXME: ",
    "BUG: "
  ],
  "todohighlight.keywords": [
    {
      "text": " NOTE:",
      "color": "#181825", // Mantle
      "backgroundColor": "#fab387" // Peach (informative, low severity)
    },
    {
      "text": " DEBUG:",
      "color": "#181825", // Mantle
      "backgroundColor": "#f5c2e7" // Pink (debugging, low-medium severity)
    },
    {
      "text": " REVIEW:",
      "color": "#181825", // Mantle
      "backgroundColor": "#f38ba8" // Red (needs review, medium severity)
    },
    {
      "text": " HACK:",
      "color": "#181825", // Mantle
      "backgroundColor": "#cba6f7" // Mauve (temporary workaround, medium severity)
    },
    {
      "text": " QUESTION:",
      "color": "#181825", // Mantle
      "backgroundColor": "#94e2d5" // Teal (needs clarification, medium severity)
    },
    {
      "text": " TODO:",
      "color": "#181825", // Mantle
      "backgroundColor": "#89b4fa" // Blue (to be done, medium-high severity)
    },
    {
      "text": " CHANGED:",
      "color": "#181825", // Mantle
      "backgroundColor": "#74c7ec" // Sapphire (recent changes, medium-high severity)
    },
    {
      "text": " OPTIMIZE:",
      "color": "#181825", // Mantle
      "backgroundColor": "#a6e3a1" // Green (optimization needed, high severity)
    },
    {
      "text": " FIXME:",
      "color": "#181825", // Mantle
      "backgroundColor": " #eba0ac" // Maroon (needs fixing, high severity)
    },
    {
      "text": " BUG:",
      "color": "#181825", // Mantle
      "backgroundColor": "#f9e2af" // Yellow (critical, highest severity)
    }
  ]
  //  Test Todo Highlight
  /*
    # NOTE: text details
    # DEBUG: text details
    # REVIEW: text details
    # HACK: text details
    # QUESTION: text details
    # TODO: text details
    # CHANGED: text details
    # OPTIMIZE: text details
    # FIXME: text details
    # BUG: text details
  */
}
