{
  // "workbench.settings.editor": "json",
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.preferredDarkColorTheme": "GitHub Dark Default",
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
  "workbench.startupEditor": "readme",
  "workbench.iconTheme": "file-icons",
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#c9d1d9",
    "editorCursor.background": "#0d1117"
  },
  "window.autoDetectColorScheme": true,
  "explorer.compactFolders": false,
  "git.autofetch": true,
  "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, 'Symbols Nerd Font'",
  "editor.fontSize": 14,
  "editor.fontLigatures": false,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "terminal.integrated.fontFamily": "'JetBrains Mono NL', 'Symbols Nerd Font'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.macOptionIsMeta": true,
  "go.toolsManagement.autoUpdate": true,
  "projectManager.git.baseFolders": ["~/Projects"],
  "projectManager.sortList": "Recent",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": true
  },
  "shellformat.path": "/usr/local/bin/shfmt",
  "shellformat.flag": "--indent 2",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "find-it-faster.general.useTerminalInEditor": true,
  "find-it-faster.general.killTerminalAfterUse": true,
  "find-it-faster.general.batTheme": "GitHub",
  "editor.lineNumbers": "relative",
  "settings.cycle": [
    {
      "id": "relativeLineNumbers",
      "overrideWorkspaceSettings": false,
      "values": [
        {
          "editor.lineNumbers": "on"
        },
        {
          "editor.lineNumbers": "relative"
        }
      ]
    }
  ],
  "todohighlight.keywords": [
    {
      "text": "DEBUG:",
      "color": "#f5c2e7", // Pink
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "REVIEW:",
      "color": "#f38ba8", // Red
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "NOTE:",
      "color": "#fab387", // Peach
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "HACK:",
      "color": "#cba6f7", // Mauve
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "TODO:",
      "color": "#89b4fa", // Blue
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "FIXME:",
      "color": "#eba0ac", // Maroon
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "BUG:",
      "color": "#f9e2af", // Yellow
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "OPTIMIZE:",
      "color": "#a6e3a1", // Green
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "CHANGED:",
      "color": "#74c7ec", // Sapphire
      "backgroundColor": "#1e1e2e" // Base
    },
    {
      "text": "QUESTION:",
      "color": "#94e2d5", // Teal
      "backgroundColor": "#1e1e2e" // Base
    }
  ]
}
